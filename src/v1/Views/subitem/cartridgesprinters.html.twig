{{ include('header.html.twig') }}
{{ include('headerForm.html.twig') }}

<table class="ui blue table">
  <thead>
    <tr>
      <th colspan="5">{{ translation.cartridges_use }}</th>
    </tr>
    <tr>
      <th>{{ translation.model }}</th>
      <th>{{ translation.type }}</th>
      <th>{{ translation.date_add }}</th>
      <th>{{ translation.date_use }}</th>
    </tr>
  </thead>
  <tbody>
    {% for key,cartridge in data.cartridges_use %}
      <tr>
        <td>
          {% if cartridge.model_url != '' %}
              <a href="{{ cartridge.model_url }}">{{ cartridge.model }}</a>
          {% else %}
              {{ cartridge.model }}
          {% endif %}
        </td>
        <td>
          {% if cartridge.type_url != '' %}
              <a href="{{ cartridge.type_url }}">{{ cartridge.type }}</a>
          {% else %}
              {{ cartridge.type }}
          {% endif %}
        </td>
        <td>{{ cartridge.date_add }}</td>
        <td>{{ cartridge.date_use }}</td>
      </tr>
    {% endfor %}
  </tbody>
</table>

