{{ include('header.html.twig') }}
{{ include('headerForm.html.twig') }}

<table class="ui blue table">
  <thead>
    <tr>
      <th>{{ translation.name }}</th>
    </tr>
  </thead>
  <tbody>
    {% for printermodel in data.printermodels|sort((a, b) => a.printermodel.name|lower <=> b.printermodel.name|lower) %}
      <tr>
        <td>
          {% if printermodel.url != '' %}
            <a href="{{ printermodel.url }}">{{ printermodel.name }}</a>
          {% else %}
            <span>{{ printermodel.name }}</span>
          {% endif %}
        </td>
      </tr>
    {% endfor %}
  </tbody>
</table>

{{ include('footer.html.twig') }}
